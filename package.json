{
  "name": "clean-react",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vineboneto/clean-react.git",
  "author": "vineboneto <vineboneto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open ",
    "husky:clean": "rimraf .config",
    "husky:install": "husky install .config/husky",
    "husky:push": "husky add .config/husky/pre-push \"yarn test:ci\"",
    "husky:commit": "husky add .config/husky/pre-commit \"yarn lint-staged && yarn test:staged\"",
    "husky:config": "(yarn husky:commit) & (yarn husky:push)",
    "husky": "(yarn husky:clean) & (yarn husky:install) & yarn husky:config",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test - --watch",
    "test:staged": "yarn test - --findRelatedTests",
    "test:ci": "yarn test - --coverage"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.2",
    "eslint": "^7.21.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.4.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
