{
  "name": "clean-react",
  "version": "1.4.1",
  "main": "index.js",
  "repository": "https://github.com/vineboneto/clean-react.git",
  "author": "vineboneto <vineboneto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --open",
    "husky:clean": "rimraf .config",
    "husky:install": "husky install .config/husky",
    "husky:push": "husky add .config/husky/pre-push \"yarn test:ci\"",
    "husky:commit": "husky add .config/husky/pre-commit \"yarn lint-staged && yarn test:staged\"",
    "husky:commit-msg": "husky add .config/husky/commit-msg \".git/hooks/commit-msg $1 \"",
    "husky:config": "(yarn husky:commit) & (yarn husky:push) & (yarn husky:commit-msg)",
    "husky": "(yarn husky:clean) & (yarn husky:install) & yarn husky:config",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --silent --runInBand --no-cache",
    "test:watch": "yarn test - --watch",
    "test:staged": "yarn test - --findRelatedTests -o",
    "test:ci": "yarn test - --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.2",
    "@types/jest": "26.0.21",
    "@types/node": "^14.11.2",
    "@types/react": "17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "5.1.3",
    "cypress": "^6.8.0",
    "eslint": "^7.9.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.1.0",
    "favicons-webpack-plugin": "^5.0.2",
    "full-icu": "^1.3.1",
    "git-commit-msg-linter": "^3.0.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "start-server-and-test": "^1.11.4",
    "style-loader": "^2.0.0",
